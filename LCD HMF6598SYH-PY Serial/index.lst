....................             c2 = TEXT[chr-0x20][i] >>(8-(y%8));                
....................          } 
....................          else { 
....................             c1 = TEXT2[chr-0x53][i] <<(y%8); 
....................             c2 = TEXT2[chr-0x53][i] >>(8-(y%8)); 
....................          } 
....................          HMF6598_Gotoxy(x,(y/8));       
....................          c1 |= HMF6598_Read_data(); 
....................          HMF6598_Write_data(c1);                
....................          HMF6598_Gotoxy(x,(y/8)+1);       
....................          c2 |= HMF6598_Read_data(); 
....................          HMF6598_Write_data(c2); 
....................          x++; 
....................       } 
....................       x++; 
....................       loop++; 
....................    } 
.................... } 
