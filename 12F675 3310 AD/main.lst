....................  
.................... void nokia_putpixel(int8 xnokia,int8 ynokia) 
.................... { 
....................    int line_y, plot_value; 
....................    byte byte_plot; 
....................    if((xnokia > 83)||(ynokia > 47)) 
....................       return; 
....................    line_y = (ynokia / 8); 
....................    plot_value = 0; 
....................    plot_value |= 1; 
....................    byte_plot = (plot_value<<(8-(ynokia%8)-1)); 
....................    nokia_gotoxy(xnokia,line_y); 
....................    nokia_write_data_inv(byte_plot); 
.................... } 
